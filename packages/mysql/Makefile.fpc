#
#   Makefile.fpc for MySql bindings
#

[targets]
units=mysql_com mysql_version mysql
programs=testdb

[dirs]
fpcdir=../..

[defaults]
defaultunits=1


[rules]
.PHONY: test examples

mysql$(PPUEXT): mysql$(PASEXT) mysql_com$(PPUEXT)

testdb$(EXEEXT): testdb$(PASEXT) mysql$(PPUEXT)

test: fpc_exes

examples: fpc_exes
