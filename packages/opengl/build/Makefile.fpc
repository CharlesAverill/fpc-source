#
#   Makefile.fpc for auto generation of OpenGL units
#

[target]
units=buildgl
programs=c_gen

[require]
packages=fcl

[default]
rule=all_units


[rules]
.PHONY: all_units linux win32 w32d w32s

all_units: linux win32

linux: c_gen$(EXEEXT) ogl_linux.gen
        c_gen ogl_linux.gen

win32: w32d w32s

w32d: c_gen$(EXEEXT) ogl_w32d.gen
        c_gen ogl_w32d.gen

w32s: c_gen$(EXEEXT) ogl_w32s.gen
        c_gen ogl_w32s.gen

