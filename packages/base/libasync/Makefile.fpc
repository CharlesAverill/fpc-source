#
#   Makefile.fpc for libasync
#

[package]
name=libasync
version=1.9.0

[target]
units=libasync

[install]
fpcpackage=y

[compiler]
sourcedir_linux=unix
sourcedir_freebsd=unix
sourcedir_darwin=unix
sourcedir_netbsd=unix
sourcedir_openbsd=unix
targetdir=.

[require]
packages=rtl

[default]
fpcdir=../../..
