#
#   Makefile.fpc for FPC demo package
#

[targets]
dirs=text graph modex win32

[require]
# Not always necessary, but saves a lot of trouble
packages=api

[install]
packagename=demo

[defaults]
defaultrule=all

[dirs]
fpcdir=..
targetdir=.

[postsettings]

[rules]
.PHONY: text graph modex win32

info:
        @echo makfeile targets are
        @echo - all
        @echo - text
        @echo - graph
        @echo - modex
        @echo - win32

