override EXEOBJECTS+=install
endif
ifeq ($(OS_TARGET),win32)
override EXEOBJECTS+=install
endif
ifeq ($(OS_TARGET),os2)
override EXEOBJECTS+=install
endif

# Clean

ifeq ($(OS_TARGET),linux)
override EXTRACLEANFILES+=install.o
endif

# Install

ZIPTARGET=install

