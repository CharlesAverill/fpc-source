  result := false;

  slHeader := TStringList.Create;
  try
    slFooter := TStringList.Create;
    try
      case aTyp of
         tfFPC: begin
                  writeln(format('outputformat: fpc  platform: %s  path: %s',
                                 [cPlatform[aX64], aDestPath]));

                  FileExt := '.pp';

