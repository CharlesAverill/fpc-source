#
# Makefile.fpc for Free Pascal testing utilities
#

[install]
fpcpackage=y

[default]
fpcdir=../..

[require]
packages = mysql

[target]
programs = dotest fail testfail digest dbdigest

[rules]

# To always clean dbdigest regardless of USESQL
clean: fpc_clean
        -$(DEL) dbdigest$(EXEEXT)

cleanall: fpc_cleanall
        -$(DEL) dbdigest$(EXEEXT)
