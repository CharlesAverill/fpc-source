pp: pp.pas \
	cobjects.ppu \
	globals.ppu \
	parser.ppu \
	systems.ppu \
	tree.ppu \
	symtable.ppu \
	options.ppu \
	link.ppu \
	import.ppu \
	files.ppu \
	verb_def.ppu \
	verbose.ppu
	ppc386 -dFPC -dGDB -dI386 pp.pas

cobjects.ppu: cobjects.pas
	ppc386 -dFPC -dGDB -dI386 cobjects.pas

globals.ppu: globals.pas \
	cobjects.ppu \
	systems.ppu
	ppc386 -dFPC -dGDB -dI386 globals.pas

systems.ppu: systems.pas
