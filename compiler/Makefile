	     $(wildcard targets/*.pas) $(wilcard targets/*.inc) \
	     $(wildcard $(PPC_TARGET)/*.pas) $(wildcard $(PPC_TARGET)/*.inc)
	$(COMPILER) pp.pas
	$(EXECPPAS)
